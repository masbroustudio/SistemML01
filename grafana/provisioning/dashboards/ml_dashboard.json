{
  "title": "dashboard - YudhaE",
  "schemaVersion": 38,
  "version": 1,
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 0,
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s","10s","30s","1m","5m"] },
  "panels": [
    {
      "type": "stat",
      "title": "Total Prediksi",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "targets": [
        { "expr": "prediction_requests_total", "legendFormat": "requests" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests per minute",
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
      "targets": [
        { "expr": "sum(rate(prediction_requests_total[1m]))", "legendFormat": "rpm" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Distribusi Kelas Prediksi",
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0},
      "targets": [
        { "expr": "sum by (class) (prediction_output_count_total)", "legendFormat": "{{class}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency (p50/p90/p99)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "targets": [
        { "expr": "histogram_quantile(0.5, sum(rate(prediction_latency_seconds_bucket[5m])) by (le))", "legendFormat": "p50" },
        { "expr": "histogram_quantile(0.9, sum(rate(prediction_latency_seconds_bucket[5m])) by (le))", "legendFormat": "p90" },
        { "expr": "histogram_quantile(0.99, sum(rate(prediction_latency_seconds_bucket[5m])) by (le))", "legendFormat": "p99" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Feature: Age Distribution",
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 6},
      "targets": [
        { "expr": "rate(feature_age_distribution_bucket[5m])", "legendFormat": "age bucket" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Feature: Fare Distribution",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 6},
      "targets": [
        { "expr": "rate(feature_fare_distribution_bucket[5m])", "legendFormat": "fare bucket" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Input Feature Sum",
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 14},
      "targets": [
        { "expr": "rate(input_feature_sum_total[1m])", "legendFormat": "sum(features)" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Invalid Requests",
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 14},
      "targets": [
        { "expr": "rate(invalid_requests_total[1m])", "legendFormat": "invalid rpm" }
      ]
    },
    {
      "type": "timeseries",
      "title": "CPU Usage (%)",
      "gridPos": {"h": 6, "w": 4, "x": 16, "y": 14},
      "targets": [
        { "expr": "system_cpu_usage_percent", "legendFormat": "cpu %" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Memory Usage (bytes)",
      "gridPos": {"h": 6, "w": 4, "x": 20, "y": 14},
      "targets": [
        { "expr": "system_memory_usage_bytes", "legendFormat": "mem bytes" }
      ]
    },
    {
      "type": "stat",
      "title": "Last Prediction",
      "gridPos": {"h": 6, "w": 4, "x": 24, "y": 14},
      "targets": [
        { "expr": "last_prediction_value", "legendFormat": "class" }
      ]
    }
  ]
}
